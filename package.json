{
  "name": "polylang",
  "displayName": "Polylang",
  "description": "Rich Polylang language support for Visual Studio Code",
  "version": "0.0.5",
  "publisher": "durudex",
  "repository": {
    "type": "git",
    "url": "https://github.com/durudex/vscode-polylang"
  },
  "bugs": {
    "url": "https://github.com/durudex/vscode-polylang/issues"
  },
  "private": true,
  "license": "MIT",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "polylang",
      "aliases": ["Polylang", "polylang"],
      "extensions": [".polylang"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "polylang",
      "scopeName": "source.polylang",
      "path": "./syntaxes/polylang.tmLanguage.json"
    }],
    "snippets": [{
      "language": "polylang",
      "path": "./snippets/polylang.json"
    }]
  }
}
